/* pages/addressList/addressList.wxss */
.container {
  padding: 10px 0;
}
.cell-address {
  padding: 10px !important;
  align-items: center;
}
.cell-address .van-cell__title {
  padding: 0 10px;
  flex: 1;
}
.cell-address .van-cell__value {
  flex: unset;
  font-size: 16px;
}
.cell-title {
  font-size: 14px;
  display: flex;
  align-items: center;
}
.cell-label {
  font-size: 12px;
}
.tag-home {
  display: inline-block;
  color: #f53e5d;
  background: #f7dadf;
  border: 1px solid #f53e5d;
  border-radius: 2px;
  font-size: 10px;
  height: 12px;
  line-height: 12px;
  padding: 0 2px;
  margin-left: 3px;
}

.container .van-goods-action {
  background-color: transparent;
}
.btn-add {
  margin: 10px;
  background: #f53e5d;
  color: #fff;
  height: 40px;
  line-height: 40px;
  border-radius: 5px;
  font-size: 14px;
  border: none;
  width: 100%;
}