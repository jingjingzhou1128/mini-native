@import "assets/fonts/iconfont.wxss";

/* 顶部导航 */
.navbar {
  background-color: #fff;
}
.navbar .weui-navigation-bar__inner .weui-navigation-bar__left {
  width: 0 !important;
}
.navbar .weui-navigation-bar__inner .weui-navigation-bar__center {
  text-align: center;
  justify-content: flex-start;
}

/**app.wxss**/
page {
  height: 100%;
}
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: #f5f5f5;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
icon {
  margin-top: -6px;
  vertical-align: middle;
}
